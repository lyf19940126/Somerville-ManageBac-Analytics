# ManageBac API token (required)
MANAGEBAC_TOKEN=

# ManageBac API base URL (required), e.g. https://api.managebac.cn
MANAGEBAC_BASE_URL=

# Homeroom advisor numeric ID used to scope cohort students (required)
HOMEROOM_ADVISOR_ID=

# Graduating year used to filter cohort students (required), e.g. 2028
TARGET_GRADUATING_YEAR=

# Academic term ID for term-based grade/attendance endpoints (required)
TERM_ID=

# Timezone for report/snapshot date rendering (optional)
REPORT_TIMEZONE=Asia/Shanghai
